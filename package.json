{
  "name": "product-hub",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite serve",
    "build": "vue-tsc -b && pnpm lint && pnpm lint:style && vite build",
    "build:test": "pnpm build --mode test",
    "build:production": "pnpm build --mode production",
    "preview": "vite preview",
    "lint": "eslint src --fix",
    "lint:style": "stylelint **/*.{vue,css,scss} --fix",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@iconify/json": "^2.2.279",
    "@vueuse/core": "^12.0.0",
    "@xterm/addon-attach": "^0.11.0",
    "@xterm/addon-clipboard": "^0.1.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-image": "^0.8.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/addon-serialize": "^0.13.0",
    "@xterm/addon-unicode11": "^0.8.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.7.8",
    "dayjs": "^1.11.13",
    "element-plus": "^2.9.0",
    "lodash-es": "^4.17.21",
    "lunar-javascript": "^1.6.13",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.8",
    "pinia-plugin-persistedstate": "^4.1.3",
    "qs": "^6.13.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@iconify/vue": "^4.1.2",
    "@types/node": "^22.10.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "commander": "^12.1.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "meow": "9.0.0",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.1.1",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "sass": "^1.82.0",
    "stylelint": "^16.11.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.17.0",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^6.0.2",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-require-transform": "^1.0.21",
    "vite-plugin-virtual-mpa": "^1.12.1",
    "vite-plugin-vue-devtools": "^7.6.7",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.10"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.13.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --cache --ignore-unknown  --write",
      "eslint --cache --cache-location node_modules/.cache/.eslintcache --fix"
    ],
    "*.{scss,html,vue,css}": [
      "prettier --cache --ignore-unknown --write",
      "stylelint --fix --allow-empty-input"
    ],
    "*.vue": [
      "prettier --cache --write",
      "eslint --cache --cache-location node_modules/.cache/.eslintcache --fix",
      "stylelint --fix --allow-empty-input"
    ],
    "*.md": [
      "prettier --cache --ignore-unknown --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --cache --write--parser json"
    ],
    "package.json": [
      "prettier --cache --write"
    ]
  },
  "volta": {
    "node": "22.11.0"
  }
}
